{
  "name": "essentials-utils",
  "version": "1.2.8",
  "description": "A powerfull js/ts functions",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./react": {
      "import": "./lib/react/index.js",
      "types": "./lib/react/index.d.ts"
    },
    "./vue": {
      "import": "./lib/vue/index.js",
      "types": "./lib/vue/index.d.ts"
    }
  },
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "ts-node src/index.ts",
    "build": "rimraf lib && tsc --declaration",
    "format": "prettier --check src",
    "format:fix": "prettier --write src",
    "lint": "eslint --ext js,ts src",
    "lint:fix": "eslint --ext js,ts src --fix",
    "prepare": "husky install",
    "postversion": "git push && git push --tags && rm -rf build/temp",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "prettier --write src",
      "eslint --ext js,ts src --fix"
    ]
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesleyara/essentials-utils.git"
  },
  "keywords": [
    "react",
    "typescript",
    "javascript",
    "and",
    "utils."
  ],
  "author": "Wesley Ara√∫jo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wesleyara/essentials-utils/issues"
  },
  "homepage": "https://github.com/wesleyara/essentials-utils#readme",
  "dependencies": {
    "crypto-js": "^4.2.0",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.2",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.0.4",
    "postcss": "^8.5.1",
    "prettier": "^2.8.0",
    "tailwindcss": "3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^5.4.8",
    "vitepress": "^1.6.3",
    "vitepress-plugin-group-icons": "^1.3.5",
    "vitest": "^1.4.0"
  }
}
